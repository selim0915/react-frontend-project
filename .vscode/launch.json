{
  "configurations": [
    {
      "name": "CHROME",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:50001",
      "webRoot": "${workspaceFolder}/react-frontend-project"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node (react-frontend-project)",
      "program": "${workspaceFolder}/react-frontend-project/server.js",
      "cwd": "${workspaceFolder}/react-frontend-project",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
